<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.miao.mapper.MiaoCommentListMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.miao.entity.MiaoCommentList">
        <id column="post_id" property="postId" />
        <result column="user_id" property="userId" />
        <result column="comment" property="comment" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        post_id AS "postId", user_id AS "userId", comment AS "comment", create_time AS "createTime"
    </sql>


    <select id="customList" resultType="cn.stylefeng.guns.modular.miao.model.result.MiaoCommentListResult" parameterType="cn.stylefeng.guns.modular.miao.model.params.MiaoCommentListParam">
        select
        <include refid="Base_Column_List"/>
        from miao_comment_list where 1 = 1
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.miao.model.params.MiaoCommentListParam">
        select
        <include refid="Base_Column_List"/>
        from miao_comment_list where 1 = 1
    </select>

    <select id="customPageList" resultType="cn.stylefeng.guns.modular.miao.model.result.MiaoCommentListResult" parameterType="cn.stylefeng.guns.modular.miao.model.params.MiaoCommentListParam">
        select
        <include refid="Base_Column_List"/>
        from miao_comment_list where 1 = 1
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.miao.model.params.MiaoCommentListParam">
        select
        <include refid="Base_Column_List"/>
        from miao_comment_list where 1 = 1
    </select>

</mapper>
