<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.rest.mapper.MiaoDeviceTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.rest.entity.RestMiaoDeviceType">
        <id column="type_id" property="typeId" />
        <result column="color" property="color" />
        <result column="type" property="type" />
        <result column="imgurl" property="imgurl" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        type_id AS "typeId", color AS "color", type AS "type", imgurl AS "imgurl", create_time AS "createTime", update_time AS "updateTime"
    </sql>


    <select id="customList" resultType="cn.stylefeng.guns.modular.rest.model.result.MiaoDeviceTypeResult" parameterType="cn.stylefeng.guns.modular.rest.model.params.MiaoDeviceTypeParam">
        select
        <include refid="Base_Column_List"/>
        from miao_device_type where 1 = 1
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.rest.model.params.MiaoDeviceTypeParam">
        select
        <include refid="Base_Column_List"/>
        from miao_device_type where 1 = 1
    </select>

    <select id="customPageList" resultType="cn.stylefeng.guns.modular.rest.model.result.MiaoDeviceTypeResult" parameterType="cn.stylefeng.guns.modular.rest.model.params.MiaoDeviceTypeParam">
        select
        <include refid="Base_Column_List"/>
        from miao_device_type where 1 = 1
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.rest.model.params.MiaoDeviceTypeParam">
        select
        <include refid="Base_Column_List"/>
        from miao_device_type where 1 = 1
    </select>

</mapper>
